int sum(int a[], int n);
int main()
{
intbs,n,a[100],i;
clrscr();
printf("Enter the number of elements \n");
scanf("%d",&n);
printf("Enter the elements\n");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
s=sum(a,n);
printf("The sum of elements is %d",s);
